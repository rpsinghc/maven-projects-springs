<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL MAP 2.0//EN" 
    "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="portal">

<resultMap id="ptInfoMap" class="java.util.HashMap">
   <result property="ptId" column="PROCESS_TEMPLATE_ID"/>
   <result property="ptName" column="PROCESS_TEMPLATE_NAME"/>
</resultMap>

   <select id="getAllInstalledTemplates" resultMap="ptInfoMap" >
        SELECT   
               PROCESS_TEMPLATE_ID,                 
               PROCESS_TEMPLATE_NAME
        FROM 
              BIZLOGIC_PROCESSTEMPLATE
	WHERE
	      STATUS = 2 OR STATUS = 4	
    </select>
    <select id="getInstalledBSoloApps" resultClass="String" >
            SELECT NAME FROM BIZSOLO_APPS	
    </select>
</sqlMap>
