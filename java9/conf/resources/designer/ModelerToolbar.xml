<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Toolbar SYSTEM "Designer.dtd" >
<Toolbar>
  <Palettes>
    <Palette mode="BizLogic">
      <Page name="Standard">
        <Description>Standard</Description>
        <Description xml:lang="ja">標準</Description>
        <Description xml:lang="zh">标准</Description>
        <Templates>
          <ComponentTemplate type="Start" name="Start">
            <NameTemplate count="false">Start</NameTemplate>
            <Description>Start</Description>
            <Description xml:lang="ja">スタート</Description>
            <Description xml:lang="zh">开始</Description>
          </ComponentTemplate>
          <ComponentTemplate type="Workstep" name="Activity">
            <NameTemplate>Activity {0}</NameTemplate>
            <Description>Activity</Description>
            <Description xml:lang="ja">アクティビティー</Description>
            <Description xml:lang="zh">活动</Description>
          </ComponentTemplate>
          <ComponentTemplate type="External" name="Adapter">
            <NameTemplate>Adapter {0}</NameTemplate>
            <Description>Adapter</Description>
            <Description xml:lang="ja">アダプター</Description>
            <Description xml:lang="zh">整合(集成)</Description>
          </ComponentTemplate>
          <ComponentTemplate type="Subprocess" name="Subprocess">
            <NameTemplate>Subprocess {0}</NameTemplate>
            <Description>Subprocess</Description>
            <Description xml:lang="ja">サブプロセス</Description>
            <Description xml:lang="zh">子流程</Description>
          </ComponentTemplate>
          <ComponentTemplate type="Decision" name="Decision">
            <NameTemplate>Decision {0}</NameTemplate>
            <Description>Decision</Description>
            <Description xml:lang="ja">条件分岐</Description>
            <Description xml:lang="zh">条件分支</Description>
          </ComponentTemplate>
          <ComponentTemplate type="AndJoin" name="And" width="40" height="40">
            <NameTemplate>And {0}</NameTemplate>
            <Description>And-join</Description>
            <Description xml:lang="ja">ＡＮＤ結合</Description>
            <Description xml:lang="zh">与</Description>
          </ComponentTemplate>
          <ComponentTemplate type="OrJoin" name="Or" width="40" height="40">
            <NameTemplate>Or {0}</NameTemplate>
            <Description>Or-join</Description>
            <Description xml:lang="ja">ＯＲ結合</Description>
            <Description xml:lang="zh">或</Description>
          </ComponentTemplate>
          <ComponentTemplate type="XorJoin" name="Xor" width="40" height="40">
            <NameTemplate>Xor {0}</NameTemplate>
            <Icon toolbar="xor.gif" diagram=""/>
            <Description>Xor-join</Description>
          </ComponentTemplate>
          <ComponentTemplate type="Split" name="Split" width="40" height="40">
            <NameTemplate>Split {0}</NameTemplate>
            <Description>Split</Description>
            <Description xml:lang="ja">分岐</Description>
            <Description xml:lang="zh">分支</Description>
          </ComponentTemplate>
          <ComponentTemplate type="End" name="End">
            <NameTemplate>End {0}</NameTemplate>
            <Description>End</Description>
            <Description xml:lang="ja">終了</Description>
            <Description xml:lang="zh">结束</Description>
          </ComponentTemplate>
        </Templates>
      </Page>
      <Page name="Notes">
        <Description>Notes</Description>
        <Description xml:lang="ja">ノート</Description>
        <Templates>
          <ComponentTemplate type="StickyNote" name="StickyNote" width="120" height="120">
            <NameTemplate count="false">Sticky note</NameTemplate>
            <Icon toolbar="stickyNote.gif" diagram=""/>
            <Description>Sticky note</Description>
            <Description xml:lang="ja">付箋紙</Description>
          </ComponentTemplate>
          <ComponentTemplate type="Annotation" name="Annotation" width="120" height="120">
            <NameTemplate count="false">Annotation</NameTemplate>
            <Icon toolbar="annotation.gif" diagram=""/>
            <Description>Annotation</Description>          
          </ComponentTemplate>
        </Templates>
      </Page>
    </Palette>
  </Palettes>
</Toolbar>
