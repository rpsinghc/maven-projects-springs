<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
  PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ReportSubjectVO">

	<resultMap id="result" class="com.worldcheck.juno.vo.component.SubjectConfigVO">
		<result property="subjectId" column="SUBJECT_ID" />
		<result property="reportSubjectName" column="SUBJECT_NAME" />


	</resultMap>

	

	<!-- Insert example, using the Account parameter class -->

	<select id="getByCRN" resultMap="result"
		parameterClass="java.lang.String">
		SELECT SUBJECT_ID,SUBJECT_NAME FROM CMS_SUBJECT WHERE CRN=#crn#

	</select>
    


</sqlMap>
