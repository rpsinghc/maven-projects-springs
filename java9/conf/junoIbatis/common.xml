<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
  PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
	<sqlMap namespace="CommonVO">
	<resultMap id="commonResult"
		class="com.worldcheck.juno.vo.common.CommonVO">
		<result property="crn" column="crn" />
		<result property="processCycleId" column="processCycleId" />
		<result property="userName" column="userName" />
		<result property="documentColumnName" column="documentColumnName" />
		<result property="documentColumnId" column="documentColumnId" />
		<result property="updateOn" column="updateOn" />
		<result property="subComponent" column="subComponent" />
		<result property="componentName" column="componentName" />
		<result property="isSubComponent" column="isSubComponent" />
		<result property="errorCode" column="errorCode" />
	</resultMap>
	
	<parameterMap id="getParameterMap" class="java.util.Map">
        
         <parameter property="crn" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
         <parameter property="processCycleId" jdbcType="INTEGER" javaType="java.lang.Integer" mode="IN" />
         
         <parameter property="componentName" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
         <parameter property="subComponent" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
         <parameter property="isSubComponent" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
         <parameter property="documentColumnName" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
         <parameter property="documentColumnId" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
         <!--<parameter property="updateOn" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />-->
         <parameter property="userCode" jdbcType="VARCHAR" javaType="java.lang.String" mode="IN" />
         <parameter property="errorCode" jdbcType="INTEGER" javaType="java.lang.Integer" mode="OUT" />
 	</parameterMap>
	
	
  <!--- Calling stored procedure --> 
      <procedure id="callMakeCopy" parameterMap="getParameterMap">
      
            { call RG_PROC_MAKE_COPY( ?, ?, ?, ?, ?, ?, ?, ?,? )} 
   </procedure>
</sqlMap>

