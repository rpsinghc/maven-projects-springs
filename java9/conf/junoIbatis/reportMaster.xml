<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
  PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<!-- useless -->
<sqlMap namespace="ReportTypeMasterVO">


	<resultMap id="result"
		class="com.worldcheck.juno.vo.master.ReportTypeMasterVO">
		<result property="reportId" column="REPORT_TYPE_MASTER_ID" />
		<result property="reportType" column="REPORT_TYPE" />

	</resultMap>

	<select id="list" resultMap="result">
		select
		REPORT_TYPE_MASTER_ID,REPORT_TYPE from CMS_REPORT_TYPE_MASTER 

	 </select>

	<select id="listReportType" resultMap="result">

		select unique rtm.REPORT_TYPE_MASTER_ID, rtm.REPORT_TYPE
		from RG_REPORT_PASSWORD_MASTER rtpassm , CMS_REPORT_TYPE_MASTER rtm
		where rtm.REPORT_TYPE_MASTER_ID not in (select rtpassm.REPORT_TYPE_ID from
		RG_REPORT_PASSWORD_MASTER rtpassm) 

	</select>

</sqlMap>