<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
  PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
  "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="DBUtils">

	<resultMap id="cntry" class="com.worldcheck.atlas.vo.task.CountrySubjectVO">
		<result property="countryId" column="countryId" />
		<result property="countryName" column="countryName" />
	</resultMap>


<!-- Added by U6027780 for CR BI-293 Start  -->
	
	<select id="getDynamicColumnData" parameterClass="java.util.Map" resultClass="java.lang.String">
		select $selectColumn$ from $tablename$
		where
    	$whrClauseKey$ = #whrClauseValue#
    	<isNotNull  property="whrClauseKey_opt">
			<isNotNull  property="whrClauseValue_opt">
				and $whrClauseKey_opt$ = #whrClauseValue_opt#
			</isNotNull>
		</isNotNull>			
	</select>
<!-- Added by U6027780 for CR BI-293 End -->

</sqlMap>